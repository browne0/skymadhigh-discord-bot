{
  "name": "skymadhigh-discord-bot",
  "version": "1.0.0",
  "description": "A discord bot for da boyz on Sky Mad High",
  "main": "index.js",
  "scripts": {
    "start":
      "babel-node app/index.js --exec babel-node --presets es2015,es2017,stage-2",
    "test":
      "nodemon app/index.js --exec babel-node --presets es2015,es2017,stage-2",
    "build":
      "cpx app/data/**/*.json dist/data -v && babel --plugins transform-es2015-modules-commonjs --presets es2017 ./app -d dist",
    "serve": "node dist/index.js"
  },
  "keywords": ["discord", "sky mad high", "malik"],
  "author": "Malik Browne",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cpx": "^1.5.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "2.7.0",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "discord.js": "^11.2.1",
    "ffmpeg-binaries": "^3.2.2-3",
    "lodash": "^4.17.4",
    "node-opus": "^0.2.7",
    "performance-now": "^2.1.0",
    "youtube-node": "^1.3.2",
    "ytdl-core": "^0.18.3"
  },
  "nodemonConfig": {
    "ignore": ["app/data/*"]
  }
}
